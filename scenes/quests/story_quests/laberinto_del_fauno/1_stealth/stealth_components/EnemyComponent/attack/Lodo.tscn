script/source = "extends CharacterBody2D

const BULLET_SPEED = 300.0
var initial_direction:Vector2 = Vector2.ZERO

func start_move(target_position: Vector2) -> void:
	initial_direction = (target_position - global_position).normalized()

func _physics_process(_delta: float) -> void:
	velocity = initial_direction * BULLET_SPEED
	move_and_slide()


func _on_area_2d_body_entered(body: Node2D) -> void:
	if body.is_in_group(\"player\"):
		body.life -= 1 
		var knockback_direcion:Vector2 = (body.global_position - global_position).normalized()
		print(knockback_direcion)
		body.apply_knockback(knockback_direcion, 150.0, 0.12)
		self.queue_free()


func _on_visible_on_screen_notifier_2d_screen_exited() -> void:
	self.queue_free()
"

