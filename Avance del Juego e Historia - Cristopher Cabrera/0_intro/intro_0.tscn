[sub_resource type="Shader" id="Shader_eo1t8"]
code = "shader_type canvas_item;

uniform sampler2D screen_texture : hint_screen_texture, filter_nearest;
uniform sampler2D noise_texture  : repeat_enable;

uniform float noise_speed = 25.0;
uniform float distortion_amount = 0.015;

void fragment() {
    vec3 noise = texture(noise_texture, SCREEN_UV + vec2(TIME / (noise_speed * 3.0))).rgb;

    vec2 distortion = (noise.rg - 0.5) * distortion_amount;

    vec3 color = texture(screen_texture, SCREEN_UV + distortion).rgb;

    COLOR.rgb = color;
}
"

